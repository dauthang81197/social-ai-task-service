{
  "name": "social-ai-task-service",
  "version": "0.0.1",
  "description": "Social ai",
  "author": "Thangdt",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "husky-run": "husky-run",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:dev": "nest build",
    "build:qa": "nest build",
    "build:staging": "nest build",
    "build:production": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:compose": "nest start --tsc --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -d src/database/ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm -- migration:create",
    "typeorm:migration:run": "npm run typeorm -- migration:run -d src/database/ormconfig.ts",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert -d src/database/ormconfig.ts",
    "migrate:create": "ts-node ./src/scripts/create-migration.ts",
    "migrate:generate": "ts-node ./src/scripts/generate-migration.ts",
    "seed:run": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d src/database/ormconfig",
    "release": "semantic-release"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@angular-devkit/core": "16.2.16",
    "@angular-devkit/schematics": "16.2.16",
    "@google/generative-ai": "^0.24.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/schedule": "2.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "10.0.0",
    "@types/aws-sdk": "^2.7.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1503.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.0",
    "bullmq": "^5.14.0",
    "cache-manager": "^5.5.1",
    "cache-manager-redis-yet": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commitizen": "^4.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nestjs-i18n": "^10.2.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "tmp": "^0.2.1",
    "ts-morph": "20.0.0",
    "typeorm": "0.3.17",
    "typeorm-extension": "^3.0.1",
    "typeorm-transactional": "^0.5.0",
    "xlsx": "^0.18.5",
    "xlsx-template": "^1.4.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.8",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.10",
    "@types/jest": "29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.4.2",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.6.1",
    "lint-staged": "^15.2.2",
    "pg-mem": "^2.6.13",
    "prettier": "^3.0.0",
    "semantic-release": "^21.0.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/my-schematics(|/.*)$": "<rootDir>/libs/my-schematics/src/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/gitlab",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ],
    "publishConfig": {
      "access": "public"
    }
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix",
      "yarn lint"
    ]
  }
}
